<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	xmlns:context="http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean id="triangle" class=" org.javabrains.shape.Triangle">
		<constructor-arg index="1" value="11"></constructor-arg>
		<constructor-arg index="0" value="12"></constructor-arg>
	</bean>
	<bean id="centerO" class="org.javabrains.shape.Point">
	    <qualifier value="circleRelated"></qualifier>
		<property name="x" value="22"></property>
		<property name="y" value="1"></property>
	</bean>
	<bean id="pointA" class="org.javabrains.shape.Point">
		<property name="x" value="1"></property>
		<property name="y" value="1"></property>
	</bean>
	<bean id="pointB" class="org.javabrains.shape.Point">
		<property name="x" value="2"></property>
		<property name="y" value="2"></property>
	</bean>
	<bean id="pointC" class="org.javabrains.shape.Point">
		<property name="x" value="3"></property>
		<property name="y" value="3"></property>
	</bean>
	<bean id="square" class="org.javabrains.shape.Square">
		<property name="pointA" ref="pointA"></property>
		<property name="pointB" ref="pointB"></property>
		<property name="pointC" ref="pointC"></property>

	</bean>
	<bean id="circle" class="org.javabrains.shape.Circle">

	</bean>

	<bean
		class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor"></bean>
	<bean
		class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"></bean>
	
	<bean class="org.springframework.beans.factory.annotation.QualifierAnnotationAutowireCandidateResolver"/>
		

</beans>